    "service",
    "endpoint",
    "http",
    "https",
    "api",
    "rest"
  ],
  "contributors": [
    {
      "name": "Cristian Alarcón",
      "email": "calarconpieriz@gmail.com"
    }
  ],
  "directories": {
    "bin": "./bin",
    "lib": "./src",
    "man": "./man"
  },
  "files": [
    "bin",
    "man",
