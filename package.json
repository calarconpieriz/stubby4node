  "bin": {
    "stubby": "bin/stubby"
  },
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/calarconpieriz/stubby4node.git"
  },
  "dependencies": {
    "@brettz9/node-static": "^0.1.1",
    "ejs": "^3.1.6",
    "isutf8": "^3.1.1",
    "js-yaml": "^4.0.0",
    "node-static": "^0.7.11"
  },
  "devDependencies": {
    "mocha": "^8.3.1",
    "semistandard": "^16.0.0",
    "sinon": "^10.0.0"
  },
  "license": "Apache-2.0",
  "engine": {
    "node": ">=6.17.1"
  },
  "semistandard": {
